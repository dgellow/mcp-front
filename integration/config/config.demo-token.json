{
  "mcpProxy": {
    "baseURL": "http://localhost:8080",
    "addr": ":8080",
    "name": "mcp-front-demo"
  },
  "mcpServers": {
    "postgres": {
      "command": "docker",
      "args": [
        "run", 
        "--rm", 
        "-i", 
        "--network", 
        "host", 
        "mcp/postgres", 
        "postgresql://testuser:testpass@localhost:15432/testdb"
      ],
      "options": {
        "authTokens": ["test-token", "demo-token"],
        "logEnabled": true
      }
    }
  }
}